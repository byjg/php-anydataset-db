{
  "name": "byjg/anydataset-db",
  "description": "Anydataset Database Relational abstraction. Anydataset is an agnostic data source abstraction layer in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\Db\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "byjg/anydataset": "5.0.0.x-dev",
    "byjg/anydataset-array": "5.0.0.x-dev",
    "byjg/uri": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5",
    "byjg/cache-engine": "5.0.0.x-dev"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "ext-oci8": "*",
    "byjg/cache-engine": "5.0.0.x-dev"
  },
  "license": "MIT"
}
