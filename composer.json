{
  "name": "byjg/anydataset-db",
  "description": "Anydataset Database Relational abstraction. Anydataset is an agnostic data source abstraction layer in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\Db\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "byjg/anydataset-array": "4.9.*",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "byjg/uri": "4.9.*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5",
    "byjg/cache-engine": "4.9.*"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "ext-oci8": "*",
    "byjg/cache-engine": "4.9.*"
  },
  "provide": {
    "byjg/anydataset-implementation": "1.0"
  },
  "license": "MIT"
}
