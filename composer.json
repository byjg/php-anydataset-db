{
  "name": "byjg/anydataset-db",
  "description": "Anydataset Database Relational abstraction. Anydataset is an agnostic data source abstraction layer in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\AnyDataset\\Db\\": "src/"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/",
        "TestDb\\": "testsdb/"
      }
    },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-pdo": "*",
    "psr/cache": "^1.0|^2.0|^3.0",
    "psr/log": "^1.1|^2.0|^3.0",
    "byjg/uri": "^6.0",
    "byjg/anydataset": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.2",
    "byjg/cache-engine": "^6.0"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "ext-oci8": "*",
    "byjg/cache-engine": "^6.0"
  },
  "provide": {
    "byjg/anydataset-implementation": "1.0"
  },
  "license": "MIT"
}
